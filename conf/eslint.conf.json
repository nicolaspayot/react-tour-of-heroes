{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "jasmine": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "globals": {
    "module": true,
    "inject": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-react": 1
  }
}
